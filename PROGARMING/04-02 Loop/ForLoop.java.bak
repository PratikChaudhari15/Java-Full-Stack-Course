class ForLoop
{
	public static void main(String []args)
	{
		for (int i=1; i<=10 ;i++)
		{
			System.out.println(i);
		}

		for (char i='A'; i<='Z';i++ )
		{
			System.out.println(i);
		}

		for (char i='Z'; i>='A';i-- )
		{
			System.out.println(i);
		}

		for (char i='0'; i<='9' ;i++ )
		{
			System.out.println(i);
		}

		for (int i=1; i<=127 ;i++ )
		{
			System.out.print(i+" : "+((char)i));//do it
			

		}
	}
}