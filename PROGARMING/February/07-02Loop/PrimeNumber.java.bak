import java.util.Scanner;
class PrimeNumber
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in); 
		System.out.print("Enter A Number : ");
		int num = sc.nextInt();
		int sum = 0;
		int count = 0;
		for (int i = 2 ;i<(num/2) ;i++ )
		{
			if (num%i==0)
			{
				count++;
			}
		}
		System.out.print(count==0?num+"is prime ":num+"Is not Prime");
	}
}